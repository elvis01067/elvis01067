      "source": [
        "# Install graphviz if needed\n",
        "!apt-get -qq install -y graphviz && pip install graphviz\n",
        "\n",
        "import graphviz\n",
        "\n",
        "# Create a Digraph\n",
        "dot = graphviz.Digraph(comment='Arquitectura')\n",
        "\n",
        "# Clientes\n",
        "dot.node('A', 'Clientes\\n(Navegador Web, App Móvil)', shape='box')\n",
        "\n",
        "# Balanceador de Carga\n",
        "dot.node('B', 'Balanceador de Carga', shape='box')\n",
        "\n",
        "# Conectar Clientes al Balanceador de Carga\n",
        "dot.edge('A', 'B')\n",
        "\n",
        "# Microservicios\n",
        "microservices = ['Autenticación', 'Catálogo de Productos', 'Pasarela de Pago', 'Atención al Cliente', 'Seguridad']\n",
        "for service in microservices:\n",
        "    dot.node(service, f'{service} Service\\n(Docker)', shape='box')\n",
        "    dot.edge('B', service)\n",
        "\n",
        "# Bases de Datos\n",
        "dot.node('PostgreSQL', 'Base de Datos\\nPostgreSQL', shape='cylinder')\n",
        "dot.node('MongoDB', 'Base de Datos\\nMongoDB', shape='cylinder')\n",
        "\n",
        "# Conectar Bases de Datos a Microservicios\n",
        "dot.edge('Pasarela de Pago', 'PostgreSQL')\n",
        "dot.edge('Autenticación', 'PostgreSQL')\n",
        "dot.edge('Catálogo de Productos', 'MongoDB')\n",
        "\n",
        "# Kubernetes\n",
        "dot.node('Kubernetes', 'Orquestación Kubernetes', shape='octagon')\n",
        "for service in microservices:\n",
        "    dot.edge('Kubernetes', service)\n",
        "\n",
        "# CI/CD Pipeline\n",
        "dot.node('CI/CD', 'Pipeline CI/CD\\n(Jenkins, GitLab CI)', shape='box')\n",
        "for service in microservices:\n",
        "    dot.edge('CI/CD', service)\n",
        "\n",
        "# Infraestructura en la Nube\n",
        "dot.node('Cloud', 'Infraestructura en la Nube\\n(AWS, GCP, Azure)', shape='cloud')\n",
        "for service in microservices:\n",
        "    dot.edge(service, 'Cloud')\n",
        "\n",
        "# Medidas de Seguridad\n",
        "dot.node('Security', 'Medidas de Seguridad\\n(Firewalls, MFA, Cifrado)', shape='lock')\n",
        "dot.edge('Security', 'Cloud')\n",
        "\n",
        "# Render and display the diagram\n",
        "dot.render('/content/arquitectura', format='png', cleanup=True)\n",
        "dot"
      ]
    }
  ]
}